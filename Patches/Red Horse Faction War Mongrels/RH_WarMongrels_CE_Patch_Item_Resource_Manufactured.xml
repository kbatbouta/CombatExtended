				<xpath>Defs/ThingDef[defName="RNMedicine_MedicBag"]/statBases</xpath>
				<value>
					<Bulk>4</Bulk>
				</value>
				</nomatch>
			</li>

			</operations>
			</match>
		</Operation>
	</Patch>